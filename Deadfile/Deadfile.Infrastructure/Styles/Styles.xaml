            BorderThickness="1,1,2,2">
            <Grid Width="{StaticResource ClientsPageButtonSize}" Height="{StaticResource ClientsPageButtonSize}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="{StaticResource ClientsPageButtonImageGridLength}" />
                    <RowDefinition Height="{StaticResource ClientsPageButtonLabelGridLength}" />
                </Grid.RowDefinitions>
                <Viewbox Grid.Row="0" Stretch="Uniform">
                    <Grid>
                        <Path
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Stroke="Transparent"
                            StrokeThickness="2">
                            <Path.Data>
                                <LineGeometry StartPoint="0 0" EndPoint="512,512" />
                            </Path.Data>
                        </Path>
                        <Path
                            x:Name="P1"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Data="{TemplateBinding behaviors:ButtonHelper.PrimaryGeometryPath}"
                            Fill="{DynamicResource BlackBrush}" />
                        <Path
                            x:Name="P2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Data="{TemplateBinding behaviors:ButtonHelper.SecondaryGeometryPath}"
                            Fill="{DynamicResource AccentColorBrush}" />
                        <Path
                            x:Name="P3"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Data="{TemplateBinding behaviors:ButtonHelper.TertiaryGeometryPath}"
                            Fill="{DynamicResource GrayBrush10}" />
                    </Grid>
                </Viewbox>
                <Label
                    x:Name="Lbl"
                    Grid.Row="1"
                    Width="{StaticResource ClientsPageButtonSize}"
                    Height="{StaticResource ClientsPageButtonLabelHeight}"
                    HorizontalAlignment="Center"
                    HorizontalContentAlignment="Center"
                    Content="{TemplateBinding Content}" />
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="B" Property="Background" Value="{StaticResource ClientsPageButtonBackgroundHover}" />
                <Setter TargetName="B" Property="BorderBrush" Value="{DynamicResource GrayBrush4}" />
                <Setter TargetName="B" Property="BorderThickness" Value="1,1,2,2" />
                <Setter TargetName="P1" Property="Fill" Value="{DynamicResource GrayBrush8}" />
                <Setter TargetName="P2" Property="Fill" Value="{DynamicResource GrayBrush8}" />
                <Setter TargetName="P3" Property="Fill" Value="{DynamicResource GrayBrush8}" />
                <Setter TargetName="Lbl" Property="Foreground" Value="{DynamicResource GrayBrush4}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="B" Property="Background" Value="{StaticResource ClientsPageButtonBackgroundHover}" />
                <Setter TargetName="B" Property="BorderBrush" Value="{DynamicResource BlackBrush}" />
                <Setter TargetName="B" Property="BorderThickness" Value="1,1,2,2" />
                <Setter TargetName="P1" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="P2" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="P3" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="Lbl" Property="FontWeight" Value="Bold" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="B" Property="Background" Value="{StaticResource ClientsPageButtonBackgroundHover}" />
                <Setter TargetName="B" Property="BorderBrush" Value="{DynamicResource GrayBrush4}" />
                <Setter TargetName="B" Property="BorderThickness" Value="2,2,1,1" />
                <Setter TargetName="P1" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="P2" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="P3" Property="Fill" Value="{DynamicResource AccentColorBrush}" />
                <Setter TargetName="Lbl" Property="FontWeight" Value="Bold" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

